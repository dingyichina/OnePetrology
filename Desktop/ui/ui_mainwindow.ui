QScrollArea#scrollArea{
    background:transparent;

    QScrollBar:horizontal{  /*//滚动条主体部分(滑道)*/
        min-height: 10;/*//滑道最小宽度*/
        min-width:100;/*//滑道最小高度*/
        border-radius:5px;/*//滑道圆角*/
        background-color:rgb(200,200,200);/*//滑轨的颜色*/
    }

    QScrollBar::handle:horizontal{  /*//滑块主体*/
        border-radius:5px;/*//滑块圆形角度*/
        height: 10;/*//滑块宽度*/
        border-radius:5px;/*//滑道圆角*/
        background-color:rgb(50,50,20);/*//滑块颜色*/
    }  

    QScrollBar::handle:horizontal:hover{  /*//鼠标放在上面时*/
        width: 10;/*//滑块宽度*/
        border-radius:5px;/*//滑道圆角*/
        background-color:rgba(0,0,0,50%);/*//滑块颜色变化*/
    }
    
}

